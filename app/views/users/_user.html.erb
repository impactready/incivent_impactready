
  <tr class="listing-item">
    <% if current_user?(user) %>
      <td>You</td>
    <% else %>
      <td><%= user.full_name %></td>
    <% end %>
    <td class="listing-i-fxl"><%= user.member_groups %></td>
    <% if current_user.master_user? %>
      <td><%= link_to 'Membership Admin', user_admin_groups_path(user) %> | <%= link_to "Delete", user, method: :delete, data: {confirm: "Are you sure?"} %></td>
    <% end %>
  </tr>